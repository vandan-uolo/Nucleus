{
  "name": "UOLO-Nucleus",
  "version": "1.2.1",
  "description": "UOLO Nucleus",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandan-uolo/Nucleus.git"
  },
  "keywords": [
    "react",
    "reactjs"
  ],
  "author": "UOLO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vandan-uolo/Nucleus/issues"
  },
  "homepage": "https://github.com/vandan-uolo/Nucleus/admin/index",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.5.0",
    "@reduxjs/toolkit": "1.6.1",
    "axios": "^0.26.1",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "node-sass": "6.0.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "15.2.0",
    "react": "17.0.2",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-dom": "17.0.2",
    "react-drag-drop-files": "^2.3.4",
    "react-json-view": "^1.21.3",
    "react-pdf": "^5.7.1",
    "react-pdf-to-image": "^1.0.2",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.0",
    "reactstrap": "8.9.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.8.0",
    "jquery": "3.6.0",
    "typescript": "4.3.5"
  }
}
